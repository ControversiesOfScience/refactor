{
  "name": "refactor",
  "version": "1.0.0",
  "description": "To view the current state of the site, go <a href=\"http://worldviewer.github.io/\">here</a>.",
  "main": "main.js",
  "scripts": {
    "build": "npm run babel && npm run scss && npm run autoprefixer",
    "babel": "babel src/js -d dist/js",
    "scss": "node-sass --output-style compressed -o dist/css src/scss lib/scss",
    "autoprefixer": "postcss -u autoprefixer -r dist/css/*"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worldviewer/worldviewer-refactor.git"
  },
  "author": "Chris Reeve",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/worldviewer/worldviewer-refactor/issues"
  },
  "homepage": "https://github.com/worldviewer/worldviewer-refactor#readme",
  "devDependencies": {
    "autoprefixer": "^6.5.4",
    "babel-cli": "^6.18.0",
    "babel-preset-latest": "^6.16.0",
    "node-sass": "^4.0.0",
    "postcss-cli": "^2.6.0"
  }
}
